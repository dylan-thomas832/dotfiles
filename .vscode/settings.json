{
    "files.associations": {
        "install-profile": "shellscript",
        "install-standalone": "shellscript",
        "vimrc*": "viml",
        "**/zfuncs/**": "shellscript"
    },
    "cSpell.words": [
        "Conda",
        "NOPYTHONRC",
        "atexit",
        "builtins",
        "cython",
        "docstrings",
        "dotbot",
        "dotfile",
        "dotfiles",
        "getenv",
        "ipython",
        "jupyter",
        "msgs",
        "mypy",
        "powerline",
        "pythonrc",
        "rlcompleter",
        "strftime",
        "submodule",
        "submodules"
    ],
    "editor.renderControlCharacters": true,
    "editor.renderIndentGuides": true,
    "editor.renderWhitespace": "all",
    "cSpell.ignoreWords": [
        "alacritty",
        "cortesi",
        "dequis",
        "distro",
        "drun",
        "gitk",
        "horsik",
        "htop",
        "libqtile",
        "nmaster",
        "qtile",
        "rasi",
        "reparenting",
        "rofi",
        "sauvage",
        "tall",
        "toscreen",
        "wmname",
        "xmonad",
        "xprop"
    ],
}
[submodule "dotbot"]
    path = meta/dotbot
    url = https://github.com/anishathalye/dotbot
[submodule "terminal/fonts"]
    path = terminal/fonts
    url = https://github.com/powerline/fonts
[submodule "terminal/base16-shell"]
    path = terminal/colors/base16-shell
    url = https://github.com/chriskempson/base16-shell
[submodule "terminal/base16-putty"]
    path = terminal/colors/base16-putty
    url = https://github.com/benjojo/base-16-putty
[submodule "terminal/colors/conemu-themes"]
    path = terminal/colors/conemu-themes
    url = git@github.com:joonro/ConEmu-Color-Themes.git
[submodule ".tmux"]
    path = terminal/tmux
    url = git@github.com:gpakosz/.tmux.git
[submodule "shells/zsh/zshrc.d/plugins/zsh-history-substring-search"]
	path = shells/zsh/plugins/plugins/zsh-history-substring-search
	url = https://github.com/zsh-users/zsh-history-substring-search
[submodule "shells/zsh/zshrc.d/plugins/zsh-syntax-highlighting"]
	path = shells/zsh/plugins/plugins/zsh-syntax-highlighting
	url = https://github.com/zsh-users/zsh-syntax-highlighting
[submodule "shells/zsh/.config/zsh/plugins/zsh-completions"]
	path = shells/zsh/plugins/plugins/zsh-completions
	url = git://github.com/zsh-users/zsh-completions.git
[submodule "shells/zsh/.config/zsh/plugins/zsh-autosuggestions"]
	path = shells/zsh/plugins/plugins/zsh-autosuggestions
	url = https://github.com/zsh-users/zsh-autosuggestions
[submodule "shells/zsh/.config/zsh/plugins/conda-zsh-completion"]
	path = shells/zsh/plugins/plugins/conda-zsh-completion
	url = https://github.com/esc/conda-zsh-completion
